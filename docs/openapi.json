  "paths": {
    "/api/bigquery/sample": {
      "get": {
        "tags": [
          "bigquery"
        ],
        "summary": "Get Sample Rows",
        "description": "Return up to 5 rows from the configured BigQuery table.\nRequires GCP_PROJECT, BIGQUERY_DATASET, and BIGQUERY_TABLE to be set.",
        "operationId": "get_sample_rows_api_bigquery_sample_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "additionalProperties": true,
                    "type": "object"
                  },
                  "type": "array",
                  "title": "Response Get Sample Rows Api Bigquery Sample Get"
                }
              }
            }
          }
        }
      }
    },
    "/api/bigquery/performance": {
      "get": {
        "tags": [
          "bigquery"
        ],
        "summary": "Get Performance",
        "description": "Return deduplicated ad performance for P1 campaigns by employee acronym.\n\n- P1: substring in ad_name (e.g. MP1, RRL - P1).\n- employee_acronym: word in adset_name, surrounded by non-letters only (e.g. _HM_ matches HM; CPA does not match CP).\nSame (ad_name, adset_name) merged: spend summed, cROAS spend-weighted average.\nRequires GCP_PROJECT, BIGQUERY_DATASET, BIGQUERY_TABLE.",
        "operationId": "get_performance_api_bigquery_performance_get",
        "parameters": [
          {
            "name": "employee_acronym",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "minLength": 1,
              "description": "Employee acronym to filter by (must appear as word in adset_name, surrounded by non-letters)",
              "title": "Employee Acronym"
            },
            "description": "Employee acronym to filter by (must appear as word in adset_name, surrounded by non-letters)"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "additionalProperties": true
                  },
                  "title": "Response Get Performance Api Bigquery Performance Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/health": {
      "get": {
        "summary": "Health",
        "description": "Health check endpoint for readiness probes.",
